{
    "name": "teto-minus",
    "description": "aeaeaeae",
    "version": "2.0.0",
    "manifest_version": 3,
    "author": "RedStone576",

    "content_scripts": [{
        "matches": ["https://tetr.io/*"],
        "js": ["./src/index.js"],
        "run_at": "document_start"
    }],
  
    "web_accessible_resources": [{
        "matches": ["https://tetr.io/*"],
        "resources": [
            "/src/**/*.js",
            "/res/**/*"
        ]
    }],
  
    "host_permissions": ["*://tetr.io/*"],
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "storage",
        "unlimitedStorage"
    ]
}
