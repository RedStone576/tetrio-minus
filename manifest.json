{
  "name": "TETRIO MINUS",
  "description": "heueueue",
  "version": "1.0.0",
  "manifest_version": 2,
  "content_scripts":
  [
    {
      "matches": ["https://tetr.io/*"],
      "js": ["index.js"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources" :
  [
    "*.js",
    "config.json",
    "/source/*.js",
    "/res/user/*.png"
  ],
  "background":
  {
    "scripts":["background.js"]
  },
  "permissions":
  [
    "*://tetr.io/*",
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "storage",
    "unlimitedStorage"
  ],
  "icons":
  {
    "128": "res/external/128.png"
  },
  "browser_action":
  {
    "default_icon":
    {
      "128": "res/external/128.png"
    }
  },
  "options_page": "source/dashboard/dashboard.html"
}
